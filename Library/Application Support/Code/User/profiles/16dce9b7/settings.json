{
    "editor.minimap.enabled": false,
    "explorer.confirmDragAndDrop": false,
    "workbench.colorTheme": "Quiet Light",
    "workbench.iconTheme": "c3typ_theme",
    "workbench.productIconTheme": "el-vsc-v1-icons",
    "remoteHub.commitDirectlyWarning": "off",
    "vsicons.dontShowNewVersionMessage": true,
    "diffEditor.renderSideBySide": false,
    "diffEditor.experimental.showMoves": true,
    "typescript.tsserver.watchOptions": {
        "watchFile": "useFsEvents"
    },
    "explorer.confirmDelete": false,
    "notebook.lineNumbers": "on",
    "git.openRepositoryInParentFolders": "never",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "catppuccin.bracketMode": "neovim",
    "editor.fontFamily": "'Fira Mono Nerd Font', Menlo, Monaco, 'Courier New', monospace",
    "editor.renderWhitespace": "trailing",
    "editor.cursorStyle": "block",
    "editor.wordWrap": "wordWrapColumn",
    "files.exclude": {
        "**/Thumbs.db": false
    },
    "editor.inlayHints.fontFamily": "'Fira Mono Nerd Font'",
    "editor.inlayHints.padding": true,
    "editor.linkedEditing": true,
    "editor.mouseWheelZoom": true,
    "editor.formatOnSave": true,
    "diffEditor.hideUnchangedRegions.enabled": true,
    "editor.suggest.localityBonus": true,
    "editor.suggest.shareSuggestSelections": true,
    "files.trimFinalNewlines": true,
    "workbench.commandPalette.experimental.suggestCommands": true,
    "workbench.startupEditor": "readme",
    "workbench.editor.autoLockGroups": {
        "default": true,
        "workbench.editorinputs.searchEditorInput": true,
        "workbench.editor.chatSession": true,
        "jupyter-notebook": true,
        "github-issues": true,
        "jsProfileVisualizer.cpuprofile.table": true,
        "jsProfileVisualizer.heapprofile.table": true,
        "hediet.vscode-drawio": true,
        "hediet.vscode-drawio-text": true,
        "mainThreadWebview-markdown.preview": true
    },
    "workbench.editor.closeEmptyGroups": false,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.perEditorGroup": true,
    "workbench.editor.pinnedTabsOnSeparateRow": true,
    "workbench.editor.revealIfOpen": true,
    "workbench.editor.splitSizing": "distribute",
    "workbench.editor.untitled.labelFormat": "name",
    "workbench.settings.editor": "json",
    "workbench.settings.openDefaultKeybindings": true,
    "workbench.settings.openDefaultSettings": true,
    "zenMode.hideStatusBar": false,
    "zenMode.hideLineNumbers": false,
    "zenMode.fullScreen": false,
    "window.density.editorTabHeight": "compact",
    "search.defaultViewMode": "tree",
    "search.searchOnTypeDebouncePeriod": 600,
    "search.showLineNumbers": true,
    "debug.allowBreakpointsEverywhere": true,
    "debug.autoExpandLazyVariables": true,
    "debug.confirmOnExit": "always",
    "debug.console.collapseIdenticalLines": false,
    "testing.showAllMessages": true,
    "scm.alwaysShowActions": true,
    "scm.alwaysShowRepositories": true,
    "extensions.experimental.deferredStartupFinishedActivation": true,
    "extensions.ignoreRecommendations": true,
    "terminal.explorerKind": "external",
    "terminal.integrated.confirmOnExit": "hasChildProcesses",
    "terminal.integrated.confirmOnKill": "always",
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.cursorBlinking": true,
    "task.allowAutomaticTasks": "off",
    "task.autoDetect": "off",
    "remote.downloadExtensionsLocally": true,
    "remote.localPortHost": "allInterfaces",
    "notebook.consolidatedRunButton": true,
    "notebook.formatOnSave.enabled": true,
    "notebook.gotoSymbols.showAllSymbols": true,
    "notebook.showFoldingControls": "always",
    "notebook.output.wordWrap": true,
    "inlineChat.mode": "preview",
    "inlineChat.showGutterIcon": true,
    "git.autofetch": true
}